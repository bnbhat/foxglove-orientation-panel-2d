{
  "name": "orientation-panel-2d",
  "displayName": "Orientation Panel 2D",
  "description": "A 2D visualization of orientation data from ROS messages with toggleable roll, pitch, and yaw displays",
  "publisher": "peek-robotics",
  "version": "0.0.6",
  "license": "MIT",
  "main": "./dist/extension.js",
  "keywords": [
    "foxglove",
    "ros",
    "orientation",
    "quaternion",
    "pitch",
    "roll",
    "yaw"
  ],
  "scripts": {
    "build": "foxglove-extension build",
    "foxglove:prepublish": "foxglove-extension build --mode production",
    "lint": "eslint .",
    "lint:ci": "eslint .",
    "lint:fix": "eslint --fix .",
    "local-install": "foxglove-extension install",
    "package": "foxglove-extension package",
    "pretest": "foxglove-extension pretest"
  },
  "devDependencies": {
    "@foxglove/eslint-plugin": "2.0.0",
    "@foxglove/extension": "2.18.1",
    "@foxglove/schemas": "^1.6.6",
    "@types/lodash": "^4.17.13",
    "@types/react": "18.3.17",
    "@types/react-dom": "18.3.5",
    "create-foxglove-extension": "1.0.4",
    "eslint": "9.17.0",
    "prettier": "3.4.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "typescript": "5.7.2"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
